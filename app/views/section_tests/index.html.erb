<h1>Listing section_tests</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Start time</th>
    <th>End time</th>
    <th>Time submitted</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @section_tests.each do |section_test| %>
  <tr>
    <td><%= section_test.name %></td>
    <td><%= section_test.description %></td>
    <td><%= section_test.start_time %></td>
    <td><%= section_test.end_time %></td>
    <td><%= section_test.time_submitted %></td>
    <td><%= link_to 'Show', section_test %></td>
    <td><%= link_to 'Edit', edit_section_test_path(section_test) %></td>
    <td><%= link_to 'Destroy', section_test, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Section test', new_section_test_path %>
